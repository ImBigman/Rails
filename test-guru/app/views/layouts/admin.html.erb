<!DOCTYPE html>
<html>
<head>
  <title>Admin panel | TestGuru</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
</head>

<body>
<%= render 'shared/header' %>
<div class="wrapper">
  <div class="content">
    <%= render 'admin/nav' %>
    <%= flash_header %>
    <%= yield %>
  </div>
  <div class="footer">
    <%= render 'shared/footer' %>
  </div>
</div>
</body>
</html>
